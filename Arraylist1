package gullu;
import java.util.ArrayList;
import java.util.Iterator;
public class Student1 {
	public void accept()
	{
		ArrayList<Student>List=new ArrayList<Student>();
		Student obj=new Student(1,"Anu",89);
		Student obj1=new Student(2,"Chirag",69);
		Student obj2=new Student(3,"Vivek",99);
		Student obj3=new Student(4,"Gurpreet",79);
		Student obj4=new Student(5,"Harry",87);
		Student obj5=new Student(6,"inder",78);
		
		Iterator itr=List.iterator();
		while(itr.hasNext()){
			System.out.println(itr.next());
		}
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
      Student1 oo=new Student1();
      oo.accept();
	}

}

